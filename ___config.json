{
  "project": {
    "name": "clickdummy-creator",
    "version": "0.1.0"
  },


  "frameworks": {
    "bootstrap": {
      "v3.x": {
        "npm": {
          "bootstrap": "^3.3.7"
        },
        "dependencies": {
          "preprocessor": "less"
        }
      },
      "v4.x": {
        "npm": {
          "bootstrap": "^4.0.0"
        },
        "dependencies": {
          "preprocessor": "sass"
        }
      }
    },
    "foundation-apps": {
      "v1.x": {
        "npm": {
          "foundation-apps": "^1.2.0"
        },
        "dependencies": {
          "preprocessor": "sass"
        }
      }
    },
    "foundation-emails": {
      "v2.x": {
        "npm": {
          "foundation-emails": "^2.2.1"
        },
        "dependencies": {
          "preprocessor": "sass"
        }
      }
    },
    "foundation-sites": {
      "v6.x": {
        "npm": {
          "foundation-sites": "^6.2.3"
        },
        "dependencies": {
          "preprocessor": "sass"
        }
      }
    }
  },


  "preprocessors": {
    "less": {
      "npm": {
        "gulp-less": "^3.3.0"
      }
    },
    "sass": {
      "npm": {
        "gulp-sass": "^2.3.2"
      }
    }
  },


  "spritegenerators": {
    "node-sprite-generator": {
      "npm": {
        "node-sprite-generator": "^0.10.2"
      }
    },
    "svg-sprite": {
      "npm": {
        "svg-sprite": "^1.3.6"
      }
    }
  },


  "paths": {
    "packageJson": "package.json",
    "template": "template",
    "frameworks": "frameworks",
    "preprocessors": "preprocessors",
    "spritegenerators": "sprite-generators",
    "recursive": "**/*",
    "src": "src",
    "assets": "src/assets"
  }
}
